{
  "manifest_version": 2,

  "name": "THON Countdown",
  "description": "Get Hype for THON",
  "author": "DJHerls",
  "version": "1.0.1",
  
"icons": {"16": "icons/icon16.png", 
		"32": "icons/icon32.png",
